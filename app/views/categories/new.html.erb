<h1>New Category</h1>


<%= form_with model: @category do |form| %>

  <div>
    <%= form.label :genre %><br>
    <%= form.text_field :genre %> <br><br>
  </div>

  <div>
    <%= form.label :image %><br>
    <%= form.file_field :image %><br><br>
<!--    <%#= form.label :image_url %><br>-->
    <%#= form.text_field :image_url %>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>
